<h3>Student Form</h3>
<%= form_for @student do |f| %>
  <<%= f.label :first_name %>
  <%= f.text_field :first_name, id: "first_name", name: "first_name" %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name, id: "last_name", name: "last_name" %><br/>
  <%= f.submit "Submit Student"%>>
<% end %>